{
  "productName": "TypingPro",
  "version": "0.2.5",
  "identifier": "com.typingpro.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "TypingPro",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis",
      "dmg",
      "deb"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": null,
      "entitlements": null
    },
    "windows": {
      "wix": {
        "language": "en-US"
      }
    }
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "process": {},
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/DeepEyeCrypto/TypingPro/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IHRhdXJpIHByaXZhdGUga2V5ClJXUlZMdUtSREdaREpndWd3UjFkTXN2N2JUTmZXVFFwMk5LdlRzQWd4QkI3L2tCbit4VzZLeHk5Cg=="
    }
  }
}