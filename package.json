    "react-dom": "^19.2.1",
    "react-router-dom": "^7.1.5",
    "tauri-plugin-store-api": "^0.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "ffmpeg-static": "^5.3.0",
    "postcss": "^8.5.6",
    "standard-version": "^9.5.0",
