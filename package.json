{
  "name": "typingpro",
  "private": true,
  "version": "0.0.39",
  "author": "TypingPro Team",
  "description": "A professional typing tutor application.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tauri": "tauri",
    "check:requirements": "node scripts/check-requirements.cjs",
    "build:frontend": "vite build",
    "build:desktop": "tauri build",
    "build:all": "npm run check:requirements && npm run build:frontend && npm run build:desktop",
