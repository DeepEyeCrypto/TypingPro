{
  "name": "typingpro",
  "private": true,
  "version": "0.0.31",
  "author": "TypingPro Team",
  "description": "A professional typing tutor application.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:all": "node scripts/build-release.cjs",
    "// NOTE": "Use 'npm run tauri:build' for releases to auto-increment version. 'tauri build' does NOT bump version.",
    "tauri:build:all": "npm run version:bump && tauri build --target x86_64-apple-darwin && tauri build --target aarch64-apple-darwin && tauri build --target x86_64-pc-windows-msvc && tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:debug": "tauri build --debug",
    "version:bump": "standard-version --release-as patch --skip.tag --skip.changelog && node scripts/sync-version.js",
    "tauri:build": "npm run version:bump && tauri build",
    "open:debug": "./scripts/open-app-debug.sh"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@tauri-apps/api": "^1.6.0",
    "firebase": "^12.6.0",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.1.5",
    "tauri-plugin-store-api": "^0.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "ffmpeg-static": "^5.3.0",
    "postcss": "^8.5.6",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepEyeCrypto/TypingPro.git"
  }
}
