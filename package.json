{
  "name": "typingpro",
  "private": true,
  "version": "0.0.0",
  "author": "TypingPro Team",
  "description": "A professional typing tutor application.",
  "license": "MIT",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:pack:mac": "npm run build && electron-builder --mac",
    "electron:pack:win": "npm run build && electron-builder --win",
    "electron:pack:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "@google/genai": "^1.31.0",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.typingpro.app",
    "productName": "TypingPro",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "preload.cjs",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "dist/logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "dist/logo.png"
    },
    "linux": {
      "maintainer": "TypingPro Team <team@typingpro.com>",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "dist/logo.png",
      "category": "Education"
    }
  }
}