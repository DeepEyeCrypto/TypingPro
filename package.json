{
  "name": "typingpro",
  "private": true,
  "version": "0.0.52",
  "author": "TypingPro Team",
  "description": "A professional typing tutor application.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tauri": "tauri",
    "check:requirements": "node scripts/check-requirements.cjs",
    "build": "vite build",
    "build:desktop": "node scripts/build-desktop.cjs",
    "clean": "rimraf dist src-tauri/target",
    "fresh-build": "npm run clean && node scripts/bump-version.js && npm run build:desktop",
    "lint": "tsc --noEmit",
    "preview": "vite preview",
    "bump": "node scripts/bump-version.js",
    "build:core": "node scripts/bump-version.js && npm run clean && npm run check:requirements && npm run build && npm run build:desktop && npm run organize-artifacts",
    "build:all": "node scripts/bump-version.js && npm run build && npm run build:desktop",
    "organize-artifacts": "node scripts/organize-artifacts.js",
    "release:bump": "standard-version --release-as patch --skip.changelog && node scripts/sync-version.js",
    "release:github": "git push --follow-tags origin main",
    "release:all": "npm run release:bump && npm run build:core && npm run release:github"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@tauri-apps/api": "^1.6.0",
    "firebase": "^12.6.0",
    "hls.js": "^1.6.15",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.1.5",
    "tauri-plugin-store-api": "^0.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "ffmpeg-static": "^5.3.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepEyeCrypto/TypingPro.git"
  }
}